<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?scenebuilder-stylesheet resources/style.css?>

<VBox alignment="CENTER" styleClass="vbox" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.mainMenuController">
   <children>
      <Button fx:id="startButton" mnemonicParsing="false" onAction="#start" text="Start" />
      <Button fx:id="exitButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#exit" stylesheets="@../style.css" text="Exit">
         <font>
            <Font size="68.0" />
         </font>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
